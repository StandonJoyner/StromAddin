<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns='http://schemas.microsoft.com/office/2009/07/customui' loadImage='LoadImage'>
  <ribbon>
    <tabs>
      <tab id='stromtab' label='Strom'>
        <group id='group1' label='My Group'>
          <button id='button1' label='My Button' onAction='OnButtonPressed'/>
          <button id='testBtn' label='TEST' onAction='OnTestButtonPressed'/>
          <button id='rtdBtn' label='RTD' onAction='OnRTDButtonPressed'/>
        </group>
        <group id='group2'>
          <button id='data_history' image='history' size='large' label='Data History' onAction='OnActionButton'/>
          <button id='real_time' image='realtime' size='large' label='Real Time' onAction='OnActionButton'/>
        </group>
        <group id='group3' label='Tools'>
          <button id='identifier_lookup' image='search' size='large' label='Identifier Lookup' onAction='OnActionButton'/>
          <button id='refresh' image='refresh' size='large' label='Refresh' onAction='OnActionButton'/>
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>
